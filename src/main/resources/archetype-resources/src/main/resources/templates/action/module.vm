#* @vtlvariable name="i18n" type="com.atlassian.jira.util.I18nHelper" *#

#set($title = "$i18n.getText('module-item.label')")

<html lang="en">
<head>
    <title>$title</title>
    <meta name="decorator" content="atl.general"/>
</head>

<body class="app-body">

<section id="content" role="main">
    #* @vtlvariable name="frontendDevMode" type="java.lang.Boolean" *#
    #if ($frontendDevMode)
        <script type="text/javascript" src="http://localhost:8080/frontend/frontend.js"/>
    #else
        #* @vtlvariable name="requestContext" type="com.atlassian.jira.util.velocity.VelocityRequestContext" *#
        <script type="text/javascript" src="$requestContext.baseUrl/download/resources/${project.groupId}.${project.artifactId}/js/frontend.min.js"/>
    #end
</section>

</body>
</html>
